#include<stdio.h>
int main()
{
    int i,n;
    printf("enter the input elements:");
    scanf("%d",&n);
    int arr1[n];
    int arr2[n];
    printf("enter %d elements for array:\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr1[i]);
    }
    for(i=0;i<n;i++)
    {
        arr2[i]=arr1[i];
    }
    printf("elements of array1:\n");
    for(i=0;i<n;i++)
    {
        printf("%d",arr1[i]);
    }
    printf("/n");
    printf("elements of array2:\n");
    for(i=0;i<n;i++)
    {
        printf("%d",arr2[i]);
    }
    printf("/n");
}
