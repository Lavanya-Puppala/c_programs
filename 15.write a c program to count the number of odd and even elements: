#include<stdio.h>
void countOddEven(int arr[],int n)
{
    int oddcount=0,evencount=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]%2==0)
        evencount++;
        else
        oddcount++;
    }
    printf("number of even elements:%d\n",evencount);
    printf("number of odd elements:%d\n",oddcount);
    }
    int main()
    {
        int arr[]={1,2,3,4,5,6,7,8,9};
        int n=sizeof(arr)/sizeof(arr[0]);
        countOddEven(arr,n);
        return 0;
    }
