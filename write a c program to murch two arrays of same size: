#include<stdio.h>
void multiplyArrays(int arr1[],int arr2[],int result[],int size)
{
    for(int i=0;i<size;i++){
        result[i]=arr1[i]*arr2[i];
    }
}
int main(){
    int size=3;
    int arr1[]={1,2,3};
    int arr2[]={4,5,6};
    int result[size];
    multiplyArrays(arr1,arr2,result,size);
    printf("result:");
    for(int i=0;i<size;i++)
    {
        printf("%d ",result[i]);
    
    }
}
